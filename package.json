{
  "name": "express-es6-rest-api",
  "version": "0.3.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "developit/express-es6-rest-api",
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.5.2",
    "morgan": "^1.8.0",
    "resource-router-middleware": "^0.6.0",
    "array-unique": "^0.3.2",
    "body-parser": "^1.15.2",
    "config": "^1.25.1",
    "es6-promise": "^4.0.5",
    "express": "^4.13.3",
    "cors": "^2.7.1",
    "jsonschema": "^1.1.1",
    "minimist": "^1.2.0",
    "mongodb": "~2.2",
    "newrelic": "~1.40.0",
    "passport": "^0.3.2",
    "redis": "^2.7.1",
    "request": "^2.79.0",
    "swagger-node-express": "^2.1.3",
    "through2": "~2.0.3",
    "unzip": "~0.1.11",
    "xml2js": "^0.4.17",
    "chai": "^4.1.0",
    "chai-http": "^3.0.0",
    "docco": "^0.7.0",
    "eslint": "^3.19.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "nsp": "^2.6.3"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  }
}
